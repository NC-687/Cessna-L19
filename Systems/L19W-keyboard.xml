<?xml version="1.0" encoding="ISO-8859-1"?>

<PropertyList>
  
  <keyboard>
    
    <key n="100">
      <name>d</name>
      <desc>Crew door</desc>
      <binding>
        <command>nasal</command>
        <script>doors.crew.toggle();</script>
      </binding>
    </key>
    
    <key n="108">
      <name>l</name>
      <desc>Landing light</desc>
      <binding>
        <command>property-toggle</command>
        <property>/controls/lighting/landing-lights</property>
      </binding>
    </key>
    
    <!-- ###### -->   
    <key n="101">
      <name>e</name>
      <desc>Rockets</desc>
      <binding>
        <command>nasal</command>
        <script>guns.fire_Rocket()</script>
      </binding>
      <mod-up>
        <binding>
          <command>nasal</command>
          <script>guns.stop_Rocket()</script>
        </binding>
      </mod-up>
    </key>
    <!-- ###### -->   
    
  </keyboard>
  
</PropertyList>
